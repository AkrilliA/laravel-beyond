{
    "name": "akrillia/laravel-beyond",
    "type": "library",
    "license": "ISC",
    "version": "5.0.0",
    "autoload": {
        "psr-4": {
            "AkrilliA\\LaravelBeyond\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jonas Regner",
            "email": "regnerjonas@protonmail.com"
        },
        {
            "name": "Alexander Gaal",
            "email": "alexander.gaal@gipfel.dev"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "illuminate/console": "^9.0",
        "illuminate/filesystem": "^9.0",
        "regnerisch/laravel-command-hooks": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AkrilliA\\LaravelBeyond\\LaravelBeyondServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "orchestra/testbench": "^7.6",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-data": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
