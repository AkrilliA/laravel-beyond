{
    "name": "regnerisch/laravel-beyond",
    "type": "library",
    "license": "ISC",
    "version": "3.0.0-beta.10",
    "autoload": {
        "psr-4": {
            "Regnerisch\\LaravelBeyond\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jonas Regner",
            "email": "regnerjonas@protonmail.com"
        },
        {
            "name": "Alexander Gaal",
            "email": "alexander.gaal@gipfel.dev"
        },
        {
            "name": "Nils Vennemann",
            "email": "nilsvennemann95@gmail.com"
        },
        {
            "name": "Mathis Haane"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "illuminate/console": "^9.0",
        "illuminate/filesystem": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Regnerisch\\LaravelBeyond\\LaravelBeyondServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "orchestra/testbench": "^7.6",
        "spatie/data-transfer-object": "^3.8",
        "spatie/laravel-query-builder": "^5.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
