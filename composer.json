{
    "name": "akrillia/laravel-beyond",
    "type": "library",
    "license": "ISC",
    "version": "7.0.0-beta.1",
    "autoload": {
        "psr-4": {
            "AkrilliA\\LaravelBeyond\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jonas Regner",
            "email": "regnerjonas@protonmail.com"
        },
        {
            "name": "Alexander Gaal",
            "email": "alexander.gaal@gipfel.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/filesystem": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AkrilliA\\LaravelBeyond\\LaravelBeyondServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "orchestra/testbench": "^8.0",
        "laravel/pint": "^1.6",
        "spatie/laravel-query-builder": "^5.1",
        "spatie/laravel-data": "^3.1",
        "spatie/laravel-queueable-action": "^2.14"
    },
    "suggest": {
        "spatie/laravel-query-builder": "This package is required for Queries.",
        "spatie/laravel-data": "This package is required for DataTransferObjects.",
        "spatie/laravel-queueable-action": "This package is required for queueable Actions."
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    }
}
