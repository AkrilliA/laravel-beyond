{
    "name": "akrillia/laravel-beyond",
    "type": "library",
    "license": "ISC",
    "version": "7.0.0-beta.8",
    "autoload": {
        "psr-4": {
            "AkrilliA\\LaravelBeyond\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jonas Regner",
            "email": "regnerjonas@protonmail.com"
        },
        {
            "name": "Alexander Gaal",
            "email": "alexander.gaal@gipfel.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "laravel/prompts": "^0.1.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AkrilliA\\LaravelBeyond\\LaravelBeyondServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "orchestra/testbench": "^8.23|^9.1",
        "laravel/pint": "^1.10|^2.0",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/laravel-data": "^3.7|^4.0",
        "spatie/laravel-queueable-action": "^2.14",
        "phpstan/phpstan": "^1.10|^2.0"
    },
    "suggest": {
        "spatie/laravel-query-builder": "This package is required for Queries.",
        "spatie/laravel-data": "This package is required for DataTransferObjects.",
        "spatie/laravel-queueable-action": "This package is required for queueable actions.",
        "qossmic/deptrac-shim": "This package can help you enforce architectural decisions by Laraval Beyond."
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan"
    },
    "minimum-stability": "dev"
}
